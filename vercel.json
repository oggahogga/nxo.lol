{
  "rewrites": [
    {
      "source": "/usercount",
      "destination": "/public/api/usercount.json"
    },
    {
      "source": "/reviews",
      "destination": "/public/reviews/index.html"
    },
    {
      "source": "/showcases",
      "destination": "/public/showcases/index.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ],
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ]
}
