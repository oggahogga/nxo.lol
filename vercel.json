{
  "rewrites": [
    {
      "source": "/reviews",
      "destination": "/reviews/index.html"
    },
    {
      "source": "/showcases",
      "destination": "/showcases/index.html"
    },
    {
      "source": "/admin",
      "destination": "/public/admin/index.html"
    },
    {
      "source": "/api/test",
      "destination": "/public/api/test.js"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ]
}
